{
  "name": "angular2-from-scratch",
  "description": "Angular2 project with webpack, typescript",
  "author": "Oshri Ifrah",
  "version": "1.0.0",
  "scripts": {
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "postinstall": "typings install",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "clean": "rimraf dist",
    "publish": "npm run build && git add dist && git commit -m \"dist subtree commit\" && git push --delete origin gh-pages && git subtree push --prefix dist origin gh-pages && npm run clean"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.5",
    "@angular/compiler": "^2.0.0-rc.5",
    "@angular/core": "^2.0.0-rc.5",
    "@angular/forms": "^0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "^2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.5",
    "@angular/platform-server": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@angular2-material/button": "2.0.0-alpha.7-4",
    "@angular2-material/button-toggle": "2.0.0-alpha.7-4",
    "@angular2-material/card": "2.0.0-alpha.7-4",
    "@angular2-material/checkbox": "^2.0.0-alpha.7-4",
    "@angular2-material/core": "2.0.0-alpha.7-4",
    "@angular2-material/grid-list": "2.0.0-alpha.7-4",
    "@angular2-material/icon": "2.0.0-alpha.7-4",
    "@angular2-material/input": "2.0.0-alpha.7-4",
    "@angular2-material/list": "2.0.0-alpha.7-4",
    "@angular2-material/menu": "2.0.0-alpha.7-4",
    "@angular2-material/progress-bar": "2.0.0-alpha.7-4",
    "@angular2-material/progress-circle": "2.0.0-alpha.7-4",
    "@angular2-material/radio": "2.0.0-alpha.7-4",
    "@angular2-material/sidenav": "2.0.0-alpha.7-4",
    "@angular2-material/slide-toggle": "2.0.0-alpha.7-4",
    "@angular2-material/slider": "2.0.0-alpha.7-4",
    "@angular2-material/tabs": "2.0.0-alpha.7-4",
    "@angular2-material/toolbar": "2.0.0-alpha.7-4",
    "@angular2-material/tooltip": "2.0.0-alpha.7-4",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "es6-shim": "^0.35.0",
    "font-awesome": "^4.6.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "url-loader": "^0.5.7",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "null-loader": "^0.1.1",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.7",
